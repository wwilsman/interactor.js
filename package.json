{
  "name": "@bigtest/interactor",
  "description": "Interaction library for testing big",
  "version": "0.9.1",
  "license": "MIT",
  "repository": "https://github.com/bigtestjs/interactor",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "docs",
    "src"
  ],
  "scripts": {
    "build": "rollup --config",
    "docs": "bigtest-docs",
