{
  "name": "interactor.js",
  "description": "Composable, immutable, asynchronous way to interact with DOM",
  "version": "1.6.1",
  "license": "MIT",
  "repository": "https://github.com/wwilsman/interactor.js",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint --ignore-path .gitignore ./",
    "start": "NODE_ENV=test karma start",
    "test": "yarn start --single-run",
    "test:jsdom": "yarn test --browsers jsdom",
    "test:coverage": "yarn test --coverage json",
    "test:jsdom:coverage": "yarn test:jsdom --coverage json",
    "postpublish": ".circleci/tag"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^26.0.1",
    "jsdom": "^16.2.2",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-jsdom-launcher": "^8.0.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "mocha": "^8.0.1",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^2.16.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "webpack": "^4.39.2"
  }
}
